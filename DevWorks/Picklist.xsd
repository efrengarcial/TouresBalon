<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://com.ibm.dwexample.bo"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:bons0="http://com.ibm.dwexample.bo">
	<xsd:include schemaLocation="Item.xsd"></xsd:include>
	<xsd:complexType name="Picklist">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="label" type="xsd:string"></xsd:element>
			<xsd:element minOccurs="0" name="identifier"
				type="xsd:string">
			</xsd:element>
			<xsd:element minOccurs="0" name="items" type="bons0:Item"
				maxOccurs="unbounded">
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>